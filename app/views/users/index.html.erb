<h1> All Users </h1>


<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user%>
      | <%= link_to "delete", user, :method => :delete, :confirm => "Confirm delete",
                                    :title => "delete #{user.name} "%>
    </li>
    <br />
    <br />
  <% end %>
</ul>